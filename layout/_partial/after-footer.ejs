<% if (config.disqus_shortname){ %>
<script>
  var disqus_shortname='<%= config.disqus_shortname %>';
  <% if (page.permalink){ %>
  var disqus_url='<%= page.permalink %>';
  <% } %>
  (function(){
    var d = document.createElement('script');
    d.type = 'text/javascript';
    d.async = true;
    d.src = '//' + disqus_shortname + '.disqus.com/<% if (page.comments) { %>embed.js<% } else { %>count.js<% } %>';
    (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(d);
  })();
</script>
<% } %>

<%- js('//cdn.bootcss.com/jquery/2.1.4/jquery.min.js') %>

<% if (theme.fancybox){ %>
  <%- css('//cdn.bootcss.com/fancybox/2.1.5/jquery.fancybox.min.css') %>
  <%- js('//cdn.bootcss.com/fancybox/2.1.5/jquery.fancybox.pack.js') %>
<% } %>

<%- js('js/script') %>
